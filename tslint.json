{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "rules":{
      "no-console":false,
      "no-debugger":false,
      "indent":[true, "spaces", 2], 
      "no-consecutive-blank-lines": [
        true,
        2
      ],
      "jsx-no-lambda": false,
      "member-access":false,
      "object-literal-sort-keys": false,
      "interface-over-type-literal":false,
      "ordered-imports": false
    },
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts"
      ]
    }
  }
  